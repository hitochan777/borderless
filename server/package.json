{
  "name": "server",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "dependencies": {
    "@types/cookie": "^0.3.3",
    "apollo-server-micro": "^2.9.0",
    "cookie": "^0.4.0",
    "firebase-admin": "^8.4.0",
    "graphql": "^14.5.3",
    "knex": "^0.19.2",
    "nexus": "^0.11.7",
    "sqlite3": "^4.1.0"
  },
  "devDependencies": {
    "micro-dev": "3.0.0",
    "ts-loader": "6.2.0",
    "ts-node": "8.3.0",
    "typescript": "3.6.3"
  },
  "scripts": {
    "dev": "TS_NODE_FILES=true node -r ts-node/register node_modules/.bin/micro-dev -p 3001 ./index.ts",
    "inspect": "node --inspect -r ts-node/register node_modules/.bin/micro-dev -p 3001 ./index.ts",
    "migrate": "ts-node -P ./tsconfig.json node_modules/.bin/knex migrate:latest --knexfile ./config/knexfile.ts",
    "seed:run": "ts-node -P ./tsconfig.json node_modules/.bin/knex seed:run --knexfile ./config/knexfile.ts",
    "generate:graphql": "ts-node -T schema.ts"
  }
}
